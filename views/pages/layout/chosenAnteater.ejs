<head>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
</head>

<body>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
    crossorigin="anonymous"></script>
  <header>
    <%- include('./../partials/header.ejs', {userInfo: userInfo, pageName: pageName}) %>
  </header>

  <a href="/" class="float-sm-start btn btn-secondary">Powrót na stronę główną</a>
  <div class="d-flex justify-content-center">
    <div class="card" style="width:500px">
      <img class="card-img-top" src="/images/<%=anteater.imageSource%>" />
      <div class="card-body">
        <h4 class="card-title">
          <%=anteater.name%>
        </h4>
        <p class="card-text">Długość mrówkojada (nie wliczając ogona) wynosi <%=anteater.length%>
            centrymentrów<br />Mrówkojad głównie występuje w państwie: <%=anteater.origin%>
        </p>
      </div>
    </div>
  </div>
</body>